{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "cortex-debug",
            "cwd": "${workspaceFolder}",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "servertype": "openocd",
            "configFiles": [
                "${workspaceFolder}/target/scripts/debug.gdb"
            ],
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "rtos": "auto",
            "linux": {
                "gdbPath": "gdb-multiarch"
            }
        }
    ]
}
